<!doctype html>
<html lang="en">

<head>
    <title>Person</title>
</head>

<body>
    <h2>Person</h2>
    <%=JSON.stringify(persons)%>
        <table border="1">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
            <%persons.forEach((p)=>{%>
                <tr>
                    <td>
                        <%=p.id%>
                    </td>
                    <td>
                        <%=p.username%>
                    </td>
                    <td>
                        <%=p.email%>
                    </td>
                </tr>
                <%})%>
        </table>
</body>

</html>